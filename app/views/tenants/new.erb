<h1>Create New Tenant</h1>

<form method = "POST" action = "/tenants/new">
<h3>Organization Information</h3>
<p><label>Name:</label><input type = "text" id = "org_name" name = "org_name"></p>
<p><label>Type:</label></p>
<input type = "radio" id = "commercial" name = "org_type" value = "Commercial">
<label for = "commercial">Commercial</label><br>
<p><input type = "radio" id = "non-profit" name = "org_type" value = "Non-Profit">
<label for = "non-profit">Non-Profit</label><br>
<p><input type = "radio" id = "internal" name = "org_type" value = "Internal">
<label for = "internal">Internal</label><br>
<p><label>Address:</label><input type = "text" id = "org_address" name = "org_address"></p>
<p><label>City:</label><input type = "text" id = "org_city" name = "org_city"></p>
<p><label>State:</label><input type = "text" id = "org_state" name = "org_state"></p>
<p><label>Zip:</label><input type = "text" id = "org_zip" name = "org_zip"></p>
<br>
<h3>Point of Contact</h3>
<p><label>Name:</label><input type = "text" id = "poc_name" name = "poc_name"></p>
<p><label>Phone:</label><input type = "text" id = "poc_phone" name = "poc_phone"></p>
<p><label>Email:</label><input type = "text" id = "poc_email" name = "poc_email"></p>
<br>
<input type = "submit" id = "submit" value = "Create Tenant">
</form>